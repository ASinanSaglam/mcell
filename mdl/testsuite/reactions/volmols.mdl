/****************************************************************************
 * Volume molecule types
 ****************************************************************************/
DEFINE_MOLECULES
{
    /* Volume molecule types used in the tests */
    V_i {DIFFUSION_CONSTANT_3D = voldiffconst}
    V_u {DIFFUSION_CONSTANT_3D = voldiffconst}
    V_b1 {DIFFUSION_CONSTANT_3D = voldiffconst}
    V_b2 {DIFFUSION_CONSTANT_3D = voldiffconst}
    V_t1   {DIFFUSION_CONSTANT_3D = voldiffconst}
    V_t2   {DIFFUSION_CONSTANT_3D = voldiffconst}
    V_t3   {DIFFUSION_CONSTANT_3D = voldiffconst}
    V_t4   {DIFFUSION_CONSTANT_3D = voldiffconst}
    V_t1to {DIFFUSION_CONSTANT_3D = voldiffconst TARGET_ONLY}
    V_t2to {DIFFUSION_CONSTANT_3D = voldiffconst TARGET_ONLY}
    V_t3to {DIFFUSION_CONSTANT_3D = voldiffconst TARGET_ONLY}
    V_t4to {DIFFUSION_CONSTANT_3D = voldiffconst TARGET_ONLY}

    /* Complex subunit states */
    V_c_su0 {DIFFUSION_CONSTANT_3D = 0}
    V_c_su1 {DIFFUSION_CONSTANT_3D = 0}

    /* Dummy molecules -- never created */
    V_a {DIFFUSION_CONSTANT_3D = voldiffconst}
    V_b {DIFFUSION_CONSTANT_3D = voldiffconst}

    /* Inert molecules */
    I_u {DIFFUSION_CONSTANT_3D = 0}
    I_b {DIFFUSION_CONSTANT_3D = 0}
    I_t {DIFFUSION_CONSTANT_3D = 0}
    I_t1 {DIFFUSION_CONSTANT_3D = 0}
    I_t2 {DIFFUSION_CONSTANT_3D = 0}
}

