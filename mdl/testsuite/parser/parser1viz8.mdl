INCLUDE_FILE = "parser1.mdl"

VIZ_OUTPUT
{
    FILENAME = "p1v8"
    MOLECULES
    {
        NAME_LIST { ALL_MOLECULES }
        ITERATION_NUMBERS
        {
            ALL_DATA     @ ALL_ITERATIONS
            POSITIONS    @ [2, 3]
            ORIENTATIONS @ [3, 4]
        }
    }
    MESHES
    {
        NAME_LIST { ALL_MESHES }
        ITERATION_NUMBERS
        {
            ALL_DATA @ [1]
            GEOMETRY @ [2, 3]
            REGION_DATA  @ [3, 4]
        }
    }
}
