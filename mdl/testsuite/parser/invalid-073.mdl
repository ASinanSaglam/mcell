/****************************************************************************
 * Parser error 073: multiple CUSTOM_RK output blocks in one file
 *
 *    Expect an error regarding the line:
 *      MODE = CUSTOM_RK
 *
 * Author: Jed Wing <jed@salk.edu>
 * Date:   2008-04-04
 ****************************************************************************/
vizdir = "dat/invalid-073/"
VIZ_DATA_OUTPUT
{
    MODE = CUSTOM_RK [[-1 TO 1 STEP 0.1]] [2, 1, -3]
    MOLECULE_FILE_PREFIX = vizdir & "molecules"
    ITERATION_LIST = [1]
}
VIZ_OUTPUT
{
    MODE = CUSTOM_RK
    MOLECULE_FILE_PREFIX = vizdir & "molecules2"
    ITERATION_LIST = [2]
}
