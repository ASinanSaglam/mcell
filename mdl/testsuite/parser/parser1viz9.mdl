INCLUDE_FILE = "parser1.mdl"

VIZ_OUTPUT
{
    FILENAME = "p1v9"
    MOLECULES
    {
        NAME_LIST { ALL_MOLECULES }
        TIME_POINTS
        {
            ALL_DATA     @ ALL_TIMES
            POSITIONS    @ [2e-6, 3e-6]
            ORIENTATIONS @ [3e-6, 4e-6]
        }
    }
    MESHES
    {
        NAME_LIST { ALL_MESHES }
        TIME_POINTS
        {
            ALL_DATA @ [1e-6]
            GEOMETRY @ [2e-6, 3e-6]
            REGION_DATA  @ [3e-6, 4e-6]
        }
    }
}
